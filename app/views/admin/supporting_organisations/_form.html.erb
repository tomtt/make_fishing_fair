<%= semantic_form_for [:admin, @supporting_organisation], builder: ActiveAdmin::FormBuilder do |f| %>
  <%= f.inputs do %>
    <%= f.input :name %>
    <%= f.input :url %>
    <% if f.object.logo.attached? %>
      <%= image_tag(f.object.logo.variant(:carousel)) %>
    <% end %>
    <%= f.input :logo, as: :file %>
    <%= f.actions %>
  <% end %>
<% end %>
